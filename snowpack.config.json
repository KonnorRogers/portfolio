{
  "scripts": {
    "mount:src": "mount src --to /_dist_",
    "mount:frontend": "mount frontend --to /_dist_",
    "mount:output": "mount output --to /",
    "build:css": "postcss",
    "build:js": "@snowpack/plugin-babel"
  },
  "buildOptions": {
    "clean": true
  },
  "devOptions": {
    "fallback": "../output/index.html",
    "out": "output"
  },
  "plugins": [
    "@snowpack/plugin-babel",
    "@snowpack/plugin-webpack"
  ]
}
